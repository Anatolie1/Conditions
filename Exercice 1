using System;

namespace Condition1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Limit 1 : ");
            int MinValue = int.Parse(Console.ReadLine());
            Console.WriteLine();

            Console.Write("Limit 2 : ");
            int MaxValue = int.Parse(Console.ReadLine());
            Console.WriteLine();

            Console.Write("Integer : ");
            int Value = int.Parse(Console.ReadLine());
            Console.WriteLine();

            if (Value > MaxValue)
            {
                Console.WriteLine("You have entered {0} which is greater than {1} which is the maximum ", Value, MaxValue);
                Console.WriteLine("Resulting integer: {0}", MaxValue);
            }
            if (Value < MinValue)
            {
                Console.WriteLine("You have entered {0} which is lower than {1} which is the minimum", Value, MinValue);
                Console.WriteLine("Resulting integer: {0}", MinValue);
            }
            Console.ReadKey();
        }
    }
}
